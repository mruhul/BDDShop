@model BddShop.Features.Registration.RegisterUser
<h2>Register</h2>
<form method="post">
    <div>
        <label asp-for="Email"></label>
        <input type="text" asp-for="Email" />
    </div>
    <div>
        <label asp-for="Password"></label>
        <input type="text" asp-for="Password" />
    </div>
    <button type="submit">Submit</button>
</form>